{
  "name": "iw-base",
  "version": "0.0.1",
  "description": "iw base package",
  "author": "Benjamin Schmitz",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Vortex375/iw.git"
  },
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts && npm run tslint && npm run copy-static-assets",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "node copyStaticAssets.js"
  },
  "dependencies": {
    "@deepstream/cache-redis": "^2.0.8",
    "@deepstream/client": "^5.0.8",
    "@deepstream/server": "^5.0.14",
    "@deepstream/types": "^2.0.9",
    "bunyan": "^1.8.12",
    "colors": "^1.4.0",
    "escape-string-regexp": "^2.0.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.3",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v16.1.0",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "@types/bunyan": "1.8.6",
    "@types/lodash": "^4.14.149",
    "@types/minimist": "^1.2.0",
    "@types/node": "^13.13.2",
    "@types/ws": "^7.2.1",
    "concurrently": "^5.1.0",
    "ts-essentials": "^6.0.0",
    "tslint": "^6.0.0",
    "typescript": "~3.8.3"
  },
  "publishConfig": {
    "directory": "dist"
  }
}
