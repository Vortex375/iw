{
  "name": "iw-base",
  "version": "0.0.1",
  "description": "iw base package",
  "author": "Benjamin Schmitz",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Vortex375/iw.git"
  },
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts && npm run tslint && npm run copy-static-assets",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "node copyStaticAssets.js"
  },
  "dependencies": {
    "@deepstream/cache-redis": "^2.0.5",
    "@deepstream/client": "^4.1.3",
    "@deepstream/server": "^4.2.5",
    "@deepstream/types": "^1.0.3",
    "bunyan": "^1.8.12",
    "colors": "^1.4.0",
    "escape-string-regexp": "^2.0.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.0",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@types/bunyan": "1.8.6",
    "@types/colors": "^1.2.1",
    "@types/lodash": "^4.14.137",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.7.2",
    "@types/ws": "^6.0.3",
    "concurrently": "^4.1.2",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "optionalDependencies": {}
}
